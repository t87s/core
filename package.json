{
  "name": "@t87s/core",
  "version": "0.0.1",
  "description": "Declarative cache invalidation that tells you when it's wrong",
  "author": "Mike Solomon <mike.solomon@hey.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t87s/core.git"
  },
  "keywords": [
    "cache",
    "caching",
    "invalidation",
    "redis",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "ms": "^2.1.3"
  },
  "peerDependencies": {
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
