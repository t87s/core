{
  "name": "@t87s/core",
  "version": "0.0.0",
  "description": "Declarative cache invalidation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "rm -rf dist",
    "demo": "cd examples/demo && pnpm start",
    "docs:dev": "cd docs && pnpm dev",
    "docs:build": "cd docs && pnpm build",
    "prepare": "husky"
  },
  "peerDependencies": {
    "ioredis": "^5.0.0",
    "@upstash/redis": "^1.34.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    },
    "@upstash/redis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^25.0.10",
    "@upstash/redis": "^1.34.0",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "ioredis": "^5.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "testcontainers": "^11.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.21.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "cache",
    "invalidation",
    "tags"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/t87s/core.git"
  }
}
